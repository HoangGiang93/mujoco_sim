<mujoco model="MuJoCo Model">
    <compiler angle="radian" autolimits="true" boundmass="0.000001" boundinertia="0.000001"/>
    <option timestep="0.005" integrator="RK4">
        <flag contact="disable" energy="enable"/>
    </option>
    <asset>
        <texture type="2d" name="grid" builtin="checker" rgb1="0.1 0.2 0.3" rgb2="0.2 0.3 0.4" width="512" height="512"/>
        <material name="grid" texture="grid" texuniform="true" reflectance="0.2"/>
        <mesh name="door_frame" file="door_frame/stl/door_frame.stl"/>
        <mesh name="door" file="door/stl/door.stl"/>
        <mesh name="door_lock" file="door_lock/stl/door_lock.stl"/>
        <mesh name="door_handle" file="door_handle/stl/door_handle.stl"/>
    </asset>
    <worldbody>
        <geom name="floor" size="0 0 0.05" type="plane" condim="4" friction="2 0.05 0.01" material="grid"/>
        <light pos="0 0 5" dir="0 0 -1" diffuse="0.5 0.5 0.5"/>
        <body name="door_frame" pos="9 2 0" gravcomp="1">
            <geom type="mesh" mesh="door_frame"/>
            <body name="door" pos="0.005215 -0.26229 0.97865" gravcomp="1">
                <joint name="door_joint" pos="0 0 0" axis="0 0 1" range="0 1.5708"/>
                <geom name="door_geom" type="mesh" mesh="door"/>
                <body name="door_lock" pos="0 0.721208 0" gravcomp="1">
                    <geom name="door_lock_geom" type="mesh" mesh="door_lock"/>
                    <body name="door_handle" pos="0 0 0.030325" gravcomp="1">
                        <joint name="door_handle_joint" pos="0 0 0" axis="1 0 0" range="0 1.5708"/>
                        <geom name="door_handle_geom" type="mesh" mesh="door_handle"/>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>
</mujoco>
